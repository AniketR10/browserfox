{
  "manifest_version": 3,
  "name": "BrowserFox",
  "version": "1.0",
  "description": "The API Client that lives in your sidebar.",
  "permissions": ["sidePanel", "storage"],
  "host_permissions": ["<all_urls>"],
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "icons": {
    "16": "vite.svg",
    "48": "vite.svg",
    "128": "vite.svg"
  }
}